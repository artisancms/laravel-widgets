{
    "name": "artisancms/widgets",
    "description": "A powerful alternative to view composers. Asynchronous widgets, reloadable widgets, console generator, caching - everything you can think of.",
    "license": "MIT",
    "keywords": ["laravel", "widgets", "ajax"],
    "authors": [
        {
            "name": "Andrew Huggins",
            "email": "andrewhuggins@gmail.com"
        }
    ],
    "homepage": "https://github.com/artisancms/widgets",
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": ">=5.1",
        "illuminate/contracts": ">=5.1",
        "illuminate/view": ">=5.1",
        "illuminate/container": ">=5.1",
        "illuminate/console": ">=5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "ArtisanCMS\\Widgets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArtisanCMS\\Widgets\\Test\\": "tests"
        }
    }
}
